{
    "manifest_version": 3,
    "name": "Custom Homepage with Quotes",
    "version": "1.0",
    "description": "Displays a custom homepage with inspirational quotes.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }